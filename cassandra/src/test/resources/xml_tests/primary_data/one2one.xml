<?xml version="1.0" encoding="UTF-8"?>

<!-- TestData are perfectly aligned to the step interval boundaries. -->

<TestCase>

  <Resource>localhost</Resource>
  <Start>300</Start>
  <End>900</End>
  <Interval>300</Interval>
  <Heartbeat>600</Heartbeat>

  <TestData>
    <Element>
      <Timestamp>0</Timestamp>
      <Name>m0</Name>
      <Value>0</Value>
    </Element>
    <Element>
      <Timestamp>0</Timestamp>
      <Name>m1</Name>
      <Value>1</Value>
    </Element>

    <Element>
      <Timestamp>300</Timestamp>
      <Name>m0</Name>
      <Value>1</Value>
    </Element>
    <Element>
      <Timestamp>300</Timestamp>
      <Name>m1</Name>
      <Value>2</Value>
    </Element>

    <Element>
      <Timestamp>600</Timestamp>
      <Name>m0</Name>
      <Value>2</Value>
    </Element>
    <Element>
      <Timestamp>600</Timestamp>
      <Name>m1</Name>
      <Value>3</Value>
    </Element>

    <Element>
      <Timestamp>900</Timestamp>
      <Name>m0</Name>
      <Value>3</Value>
    </Element>
    <Element>
      <Timestamp>900</Timestamp>
      <Name>m1</Name>
      <Value>4</Value>
    </Element>
  </TestData>

  <Expected>
    <Element>
      <Timestamp>300</Timestamp>
      <Name>m0</Name>
      <Value>1</Value>
    </Element>
    <Element>
      <Timestamp>300</Timestamp>
      <Name>m1</Name>
      <Value>2</Value>
    </Element>

    <Element>
      <Timestamp>600</Timestamp>
      <Name>m0</Name>
      <Value>2</Value>
    </Element>
    <Element>
      <Timestamp>600</Timestamp>
      <Name>m1</Name>
      <Value>3</Value>
    </Element>

    <Element>
      <Timestamp>900</Timestamp>
      <Name>m0</Name>
      <Value>3</Value>
    </Element>
    <Element>
      <Timestamp>900</Timestamp>
      <Name>m1</Name>
      <Value>4</Value>
    </Element>
  </Expected>

</TestCase>